msgid ""
      "<![CDATA[<target name=\"schemaupdate\">\n"
      "    <taskdef name=\"schemaupdate\"\n"
      "        classname=\"org.hibernate.tool.hbm2ddl.SchemaUpdateTask\"\n"
      "        classpathref=\"class.path\"/>\n"
      "    \n"
      "    <schemaupdate\n"
      "        properties=\"hibernate.properties\"\n"
      "        quiet=\"no\">\n"
      "        <fileset dir=\"src\">\n"
      "            <include name=\"**/*.hbm.xml\"/>\n"
      "        </fileset>\n"
      "    </schemaupdate>\n"
      "</target>]]>"
