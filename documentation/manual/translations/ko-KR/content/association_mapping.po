      "    <join table=\"PersonAddress\" \n"
      "        optional=\"true\">\n"
      "        <key column=\"personId\" \n"
      "            unique=\"true\"/>\n"
      "        <many-to-one name=\"address\"\n"
      "            column=\"addressId\" \n"
      "            not-null=\"true\"\n"
      "            unique=\"true\"/>\n"
      "    </join>\n"
      "</class>\n"
      "\n"
      "<class name=\"Address\">\n"
      "    <id name=\"id\" column=\"addressId\">\n"
      "        <generator class=\"native\"/>\n"
      "    </id>\n"
      "    <join table=\"PersonAddress\" \n"
      "        optional=\"true\"\n"
      "        inverse=\"true\">\n"
      "        <key column=\"addressId\" \n"
